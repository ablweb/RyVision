{
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/about",
      "dest": "/about/index.html"
    },
    {
      "src": "/about/",
      "dest": "/about/index.html"
    },
    {
      "src": "/daily",
      "dest": "/daily/index.html"
    },
    {
      "src": "/daily/",
      "dest": "/daily/index.html"
    }
  ]
}
